@await Component.InvokeAsync(nameof(SiteMeta))
@using PlacovuCMS.Core.Utility
@using Microsoft.Extensions.Options
@inject IOptions<AppConfig> AppConfigOptions
@{

    var appendScriptVersion = AppConfigOptions.Value.AppendCssScriptVersion && (AppConfigOptions.Value.ApplyCacheAfter > DateTime.Now);
    var host = $"{Context.Request.Scheme}://{Context.Request.Host}";
    var keyContext = "@context";
    var keyType = "@type";
    var keyId = "@id";
    var imgRootPath = $"{host}/upload/logo/favicon-192x192.png";
}
<script src="https://www.googleoptimize.com/optimize.js?id=OPT-KGF75JK"></script>

<link rel="stylesheet" id="et-builder-googlefonts-cached-css" href="https://fonts.googleapis.com/css?family=Roboto:100,100italic,300,300italic,regular,italic,500,500italic,700,700italic,900,900italic&amp;subset=latin,latin-ext&amp;display=swap" type="text/css" media="all">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
<link href="~/site/vendor/icofont/icofont.min.css" rel="stylesheet" asp-append-version="@appendScriptVersion">
<link href="~/site/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
<link href="~/site/vendor/venobox/venobox.min.css" rel="stylesheet">
<link href="~/site/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
<link href="~/site/vendor/aos/aos.min.css" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.css" integrity="sha512-6S2HWzVFxruDlZxI3sXOZZ4/eJ8AcxkQH1+JjSe/ONCEqR9L4Ysq5JdT5ipqtzU7WHalNwzwBv+iE51gNHJNqQ==" crossorigin="anonymous" />

<link href="~/site/css/style.css" rel="stylesheet" />
<link href="~/site/css/site.css" rel="stylesheet" />

<!-- Google "Structured" Data -->
<script type="application/ld+json">
        {
            "@keyContext":"https://schema.org",
            "@keyType":"LocalBusiness",
            "address":{	"@keyType":"PostalAddress", "addressLocality":"Colorado Springs","addressRegion":"CO", "streetAddress":"414 West Colorado Avenue Colorado Springs, CO 80905", "addressCountry": "US"}, "@keyId": "@host", "description":"Colorado Springs most reliable glass company for mirrors, showers, windows, storefronts and more! City Glass Company provides home glass repair and many other services to ensure you have the look and feel that you want in the place you call home","name":"City Glass Company","image":"@Url.Content($"{imgRootPath}/favicon-192x192.png")","priceRange":"$$$ +","telephone":"+1 (719) 634–289"
        }
</script>
<script type="application/ld+json">
        {
            "@keyContext": "https://schema.org",
            "@keyType": "Organization",
            "url": "@host",
            "logo": "@Url.Content(imgRootPath)"
        }
</script>
<!-- End Google "Structured" Data -->
