@model ContactUsConfigViewModel
@{
    ViewData["Title"] = "All contact Us Config";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row mb-4">
    <div class="col-8">
        <h1 class="f-26">All Contact Us Config</h1>
    </div>
    <div class="col-4 text-right">
        <a href="@Url.Action("Add", "ContactUsConfig", new { Area = "" })" title="Add contact Us Config" class="btn btn-primary">Add Contact Us Config</a>
    </div>
</div>

@Html.RenderAlert("ContactUsConfigMessage")

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
                <table id="dataTablesContactUsConfig" class="table table-hover" style="width:100%"></table>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/admin/js/contact-us-config.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            ContactUsConfig.LoadDataTables('dataTablesContactUsConfig', App.DisplayLength(), ("/ContactUsConfig/GetDataAsync"));
        });
    </script>
}
